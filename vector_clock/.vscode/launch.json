  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch ky store",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/key-value-store/cmd/api.go",
      "showLog": true
    },
    {
      "name": "Launch ky node1",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/key-value-node/cmd/api.go",
      "showLog": true,
      "args": ["--node-id=node1", "--address=localhost", "--port=8081"]
    },
    {
      "name": "Launch ky node2",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/key-value-node/cmd/api.go",
      "showLog": true,
      "args": ["-node-id=node2", "-address=localhost", "-port=8082"]
    },
    {
      "name": "Launch ky node3",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/key-value-node/cmd/api.go",
      "showLog": true,
      "args": ["-node-id=node3", "-address=localhost", "-port=8083"]
    }
  ]
}
